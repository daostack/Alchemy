      "fields": [
        {
          "label": "QuestionId",
          "name": "questionId",
          "placeholder":"QuestionId (0x755...)"
        },
        {
          "label": "Answer",
          "name": "answer",
          "placeholder":"Answer Hash (0x755...)"
          
        },
        {
          "label": "Answerer",
          "name": "answerer",
          "placeholder": "Address (0x0000…)"
        }
      ],
      "abi": {
        "constant": false,
        "inputs": [
          {
            "name": "questionId",
            "type": "bytes32"
          },
          {
            "name": "answer",
            "type": "bytes32"
          },
          {
            "name": "answerer",
            "type": "address"
          }
        ],
        "name": "submitAnswerByArbitrator",
        "outputs": [],
        "payable": false,
        "stateMutability": "nonpayable",
        "type": "function"
      }
    },
